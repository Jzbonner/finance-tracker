{
  "name": "finance-tracker",
  "version": "1.0.0",
  "description": "Web-App for finance instrument tracking",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run-script start:server\" \"npm run-script start:client\"",
    "stop": "taskkill -F -IM node.exe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jzbonner/finance-tracker.git"
  },
  "keywords": [
    "monngodb",
    "express",
    "react",
    "node"
  ],
  "author": "Jzbonner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jzbonner/finance-tracker/issues"
  },
  "homepage": "https://github.com/Jzbonner/finance-tracker#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.3.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1"
  },
  "devDependencies": {}
}
